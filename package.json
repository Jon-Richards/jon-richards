  "scripts": {
    "test": "jest",
    "test:silent": "jest --silent",
    "build:dev": "webpack --config=./webpack/dev.js --env.NODE_ENV=development --clean",
    "build:dist": "webpack --config=./webpack/dist.js --env.NODE_ENV=production --clean",
    "start:dev": "webpack-dev-server --config=./webpack/dev.js --env.NODE_ENV=development",
    "start:dist": "webpack-dev-server --config=./webpack/dist.js --env.NODE_ENV=production",
    "start": "npm-run-all -p api start:dev",
    "tsc": "tsc && echo Type Check Complete - No errors found. || echo Type Check Complete - See above.",
    "tslint": "tslint",
    "prettier:write": "prettier --write \"./dev/**/*.{ts,tsx,scss}\" && echo All is pretty!",
    "prettier:check": "prettier --check \"./dev/**/*\" && echo All is pretty! || echo Found prettier errors - See above.",
    "api": "json-server --watch ./test/json-server/api.json --port 3004 --static ./test/content/",
    "browserslist": "browserslist"
  },
