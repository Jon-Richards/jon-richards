{
    "extends": "gts/tslint.json",
    "rules": {
        "completed-docs": true,
        "max-line-length": [
            true,
            {
                "severity": "warning",
                "limit": 100,
                "ignore-pattern": "^import |^export {(.*?)}"
            }
        ],
        "no-consecutive-blank-lines": [true],
        "indent": [true, "spaces", 4]
    }
}
